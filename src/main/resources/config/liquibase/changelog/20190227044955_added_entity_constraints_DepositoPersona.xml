<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity DepositoPersona.
    -->
    <changeSet id="20190227044955-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="usuario_id"
                                 baseTableName="deposito_persona"
                                 constraintName="fk_deposito_persona_usuario_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuario"/>

        <addForeignKeyConstraint baseColumnNames="banco_id"
                                 baseTableName="deposito_persona"
                                 constraintName="fk_deposito_persona_banco_id"
                                 referencedColumnNames="id"
                                 referencedTableName="banco"/>

        <addForeignKeyConstraint baseColumnNames="moneda_id"
                                 baseTableName="deposito_persona"
                                 constraintName="fk_deposito_persona_moneda_id"
                                 referencedColumnNames="id"
                                 referencedTableName="moneda"/>

    </changeSet>
</databaseChangeLog>
